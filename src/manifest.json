{
        "name": "RagTab",
        "version": "0.0.3",
        "manifest_version": 2,
        "description": "A simple, open-source, configurable new-tab page",
        "icons": {
                "512": "ragtab.png"
        },
        "chrome_url_overrides": {
                "newtab": "newtab.html"
        },
        "permissions": [
                "https://www.fire.ca.gov/umbraco/api/IncidentApi/List",
                "storage"
        ],
        "browser_specific_settings": {
                "gecko": {
                        "id": "ragtab@scanuproductions.com"
                }
        }

}
